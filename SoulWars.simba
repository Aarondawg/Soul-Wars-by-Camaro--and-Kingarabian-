function GameLoop: Boolean; //Gets repeated as long as we are in the game
var
  TP : TPoint;

begin
  if not (InGame) then Exit;
  Result := True;
  if (InGraveYard) then
  begin
    TP := GetGraveYardDoor;
    if (TP.X > 0) then
    begin
      Mouse(TP.X + 1, TP.Y - 4, 2, 1, True);
      Wait(750 + Random(640));
      FFlag(0);
      Wait(750 + Random(640));
      if (DetermineTeamToJoin = 'Red') then
        FindRedBarrier
      else
        FindBlueBarrier;
    end else
    begin
      if (DetermineTeamToJoin = 'Red') then
      begin
        Walking(2501415, 17, Point(MMCX + 25, MMCY), 2, 2);
        FindRedBarrier;
      end else
      begin
        Walking(2501415, 17, Point(MMCX - 25, MMCY), 2, 2);
        FindBlueBarrier;
      end;
    end;
  end;
end;


